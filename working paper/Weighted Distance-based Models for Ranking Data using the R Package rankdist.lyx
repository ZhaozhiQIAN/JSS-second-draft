\begin_inset Float figure
placement h
wide false
sideways false
status open

\begin_layout Plain Layout
\align center
\begin_inset Graphics
	filename ../JSS/JSS/export/insert2.png
	lyxscale 50
	width 8cm

\end_inset


\end_layout

